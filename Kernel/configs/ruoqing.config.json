[
    {
        "kernelSource": {
            "name": "k30u",
            "repo": "https://github.com/ruoqing501/xiaomi_kernel_odin.git",
            "branch": "master",
            "device": "odin",
            "defconfig": "odin_defconfig"
        },
        "toolchains": [
            {
                "repo": https://github.com/ruoqing501/clang-r383902b1.git",
                "branch": "main",
                "name": "clang",
                "binaryEnv": [
                    "bin"
                ]
            }
        ],
        "enableCcache": true,
        "params": {
            "ARCH": "arm64",
            "CC": "clang",
            "externalCommands": {
                "CLANG_TRIPLE": "aarch64-linux-gnu-"
                "LD": "ld.lld"
            }
        },
        "AnyKernel3": {
            "use": true,
            "release": true
        },
        "enableKernelSU": false,
        "enableLXC": false
    }
]
